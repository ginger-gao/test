PROCEDURE FOJOBFUNCTION_SAVE_PROC (
    IN  I_TABLE TABLE(CUST_INTEGRATIONKEY NVARCHAR(128),
                     EXTERNALCODE NVARCHAR(128),
                     CUDKEY NVARCHAR(1),
                     RECORD NCLOB              
    ),
    OUT O_TABLE TABLE(CUST_INTEGRATIONKEY NVARCHAR(128),
                     EXTERNALCODE NVARCHAR(128),
                     CUDKEY NVARCHAR(1)             
    )
)
LANGUAGE SQLSCRIPT
SQL SECURITY INVOKER 

AS
BEGIN

    DECLARE v_cnt BIGINT;

    SELECT COUNT(*)
    INTO   v_cnt
    FROM   :I_TABLE
    ;

    O_TABLE_NEW =   SELECT  A.CUST_INTEGRATIONKEY
                        ,A.EXTERNALCODE
                        ,A.CUDKEY
                        ,A.RECORD
                FROM    :I_TABLE A
                LEFT OUTER JOIN FOJOBFUNCTION B
                ON A.CUST_INTEGRATIONKEY = B.CUST_INTEGRATIONKEY
                WHERE B.CUST_INTEGRATIONKEY IS NULL
                WITH HINT (NO_INLINE)
                ;

    O_TABLE_OLD =   SELECT  A.CUST_INTEGRATIONKEY
                        ,A.EXTERNALCODE
                        ,A.CUDKEY
                        ,A.RECORD
                FROM    :I_TABLE A
                INNER JOIN FOJOBFUNCTION B
                ON A.CUST_INTEGRATIONKEY = B.CUST_INTEGRATIONKEY
                WITH HINT (NO_INLINE)
                ;

    INSERT INTO FOJOBFUNCTION(CUST_INTEGRATIONKEY,EXTERNALCODE,CUDKEY,RECORD,CREATEDDATETIME,LASTMODIFIEDDATETIME,CREATEDBY,LASTMODIFIEDBY)
    SELECT CUST_INTEGRATIONKEY, EXTERNALCODE, CUDKEY, RECORD, CURRENT_TIMESTAMP, CURRENT_TIMESTAMP, CURRENT_USER, CURRENT_USER FROM :O_TABLE_NEW
    ;

	IF NOT IS_EMPTY(:O_TABLE_OLD) THEN
        MERGE INTO FOJOBFUNCTION A USING :O_TABLE_OLD B 
            ON A.CUST_INTEGRATIONKEY = B.CUST_INTEGRATIONKEY
        WHEN MATCHED THEN UPDATE SET A.EXTERNALCODE = B.EXTERNALCODE, A.RECORD = B.RECORD, A.CUDKEY = B.CUDKEY, A.LASTMODIFIEDDATETIME = CURRENT_TIMESTAMP, A.LASTMODIFIEDBY = CURRENT_USER
        ;
    END IF;

    O_TABLE =  SELECT  CUST_INTEGRATIONKEY
                        ,EXTERNALCODE
                        ,CUDKEY    
                FROM    :O_TABLE_OLD
               UNION ALL
               SELECT  CUST_INTEGRATIONKEY
                        ,EXTERNALCODE
                        ,CUDKEY   
                FROM    :O_TABLE_NEW 
            ;                
END